include(boontook-latin-classes.fea)

lookup LatinAbove.mkmk {
  markClass @COMB.abv <anchor 0 0> @LatinAbove.mkmk;
  pos mark @COMB.abv <anchor -27 210> mark @LatinAbove.mkmk;
} LatinAbove.mkmk;

lookup LatinBelow.mkmk {
  markClass @COMB.blw <anchor -60 0> @LatinBelow.mkmk;
  pos mark @COMB.blw <anchor -93 -210> mark @LatinBelow.mkmk;
} LatinBelow.mkmk;

feature mkmk {
  lookup LatinAbove.mkmk;
  lookup LatinBelow.mkmk;
} mkmk;
